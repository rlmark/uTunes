<table> <!-- need a bool for display_in_shop -->
  <tr>
    <th>Image</th>
    <th>Artist</th>
    <th>Album</th>
    <th>Price</th>
    <th>Stock</th>
    <th>Edit/Delete options</th>
  </tr>
   <% @merchant_dash.products.each do |product| %>
      <tr>
      <td><img src=<%=product.image%> width="50"/></td>
      <td><%=product.artist%></td>
      <td><%=product.album_name%></td>
      <td><%=product.price%></td>
      <td><%=product.stock%></td>
      <td>
        <ul>
          <li>Edit</li>
          <li>Delete</li>
        </ul>
      </td>
      </tr>
    <% end %>
</table>
